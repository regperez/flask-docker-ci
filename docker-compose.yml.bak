name: flask-docker-ci
services:
    flask-app:
        cpu_shares: 90
        command: []
        container_name: flask-app
        deploy:
            resources:
                limits:
                    memory: "8199864320"
        hostname: flask-app
        image: rperez19/flask-hello:latest
        networks:
            default: null
        ports:
            - mode: ingress
              target: 5000
              published: "5000"
              protocol: tcp
        restart: unless-stopped
networks:
    default:
        name: flask-docker-ci_default
x-casaos:
    author: self
    category: self
    hostname: 192.168.0.100
    icon: ""
    index: /
    is_uncontrolled: false
    port_map: "5000"
    scheme: http
    title:
        custom: Flask-app
